{
    "name": "hbynlsl/vue-think",
    "description": "vue-thinkphp api application",
    "type": "project",
    "keywords": [
        "thinkphp",
        "vue",
        "api"
    ],
    "homepage": "http://vue-think.hbynlsl.cn/",
    "authors": [
        {
            "name": "hbynlsl",
            "email": "hbynlsl@hotmail.com"
        }
    ],
    "require": {
